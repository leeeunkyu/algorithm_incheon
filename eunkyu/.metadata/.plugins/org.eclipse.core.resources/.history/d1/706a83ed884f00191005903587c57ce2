package algorithm;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.LinkedList;
import java.util.Queue;

public class Main_1941 {
	
	static int n = 5;
	static char[][] arr;
	static int size;
	
	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		Queue<People_1941> peopleq = new LinkedList<People_1941>();
		
		for (int i = 0; i < n; i++) {
			String str = br.readLine();
			for (int j = 0; j < n; j++) {
				char val = str.charAt(j);
				arr[i][j] = val;
				if(val == 'S') {
					peopleq.add(new People_1941(i, j));
				}
			}
		}
		size = peopleq.size();
		People_1941[] peopels = new People_1941[size];
		for (int i = 0; i < size; i++) {
			peopels[i] = peopleq.poll();
		}
		
		//7명중에 4명 + 5명 + 6명을 뽑는경우의 가지
		for (int i = 0; i < (1 << (size)); i++) {
			
		}
	}
}

class People_1941 {
	int y;
	int x;
	
	public People_1941(int y, int x) {
		super();
		this.y = y;
		this.x = x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
}