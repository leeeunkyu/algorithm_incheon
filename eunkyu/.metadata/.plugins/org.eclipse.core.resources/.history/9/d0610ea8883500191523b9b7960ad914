package algorithm;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

//초기 상태에서 줄기세포는 비활성 상태
//생명력 수치 X -> X시간동안 비 활성 그 후 활성
//활성 후 X시간이 지남 죽음 -> 죽은상태여도 그리드 크기는 차지
//1시간동안 상 하 좌 우 번식
//번식하는 방향에 이미 세포가 있으면 추가적으로 번식X
public class Solution_5653 {
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int testCase = Integer.parseInt(br.readLine());
		String[] info = br.readLine().split(" ");
		int n = Integer.parseInt(info[0]); //세로크기
		int m = Integer.parseInt(info[1]); //가로크기
		int k = Integer.parseInt(info[2]); //시간
		
		int[][] arr = new int[n][m];
		for (int i = 0; i < n; i++) {
			String[] arrInfo = br.readLine().split(" ");
			for (int j = 0; j < m; j++) {
				arr[i][j] = Integer.parseInt(arrInfo[j]);
			}
		}
	}
}
