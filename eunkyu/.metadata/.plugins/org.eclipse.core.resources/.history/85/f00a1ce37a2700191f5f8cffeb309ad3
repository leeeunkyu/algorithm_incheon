package algorithm;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.LinkedList;
import java.util.Queue;

public class Solution_5658 {
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int testCase = Integer.parseInt(br.readLine());
		for (int i = 1; i <= testCase; i++) {
			String str[] = br.readLine().split(" ");
			int n = Integer.parseInt(str[0]); //num길이
			int k = Integer.parseInt(str[1]); //k번째로 큰 숫자
			int num = Integer.parseInt(br.readLine()); //4의 배수 8~28 사이 각각 0이상 f이하
			
			int sideNum = num / 4;//한변에 들어갈 수 있는 숫자길이
			Queue<Integer> side = new LinkedList<Integer>();
			Queue<Integer> sideList[] = new Queue[4];
			
			
		}
	}
}
