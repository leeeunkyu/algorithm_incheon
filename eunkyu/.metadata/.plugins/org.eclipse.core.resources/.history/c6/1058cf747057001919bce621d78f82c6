package algorithm;

import java.io.InputStreamReader;

import java.io.BufferedReader;
import java.io.IOException;

public class Solution_8 {

	static int n;
	static int w;
	static int h;
	static int[][] arr;
	
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int testCase = Integer.parseInt(br.readLine());
		
		for (int tc = 0; tc < testCase; tc++) {
			String[] str = br.readLine().split(" ");
			n = Integer.parseInt(str[0]);
			w = Integer.parseInt(str[1]);
			h = Integer.parseInt(str[2]);
			arr = new int[h][w];
			
			for (int i = 0; i < h; i++) {
				String[] info = br.readLine().split(" ");
				for (int j = 0; j < w; j++) {
					arr[i][j] = Integer.parseInt(info[j]);
				}
			}
			
			goGame(0);
		}
	}

	private static void goGame(int cnt) {
		if(cnt == n) {
			//³²Àº °¹¼ö 
			return;
		}
		for (int i = 0; i < w; i++) {
			drop(i);
			setMap();
			goGame(cnt + 1);
		}
	}

	private static void setMap() {
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				
			}
		}
	}

	private static void drop(int i) {
		for (int y = 0; y < h; y++) {
			if(arr[y][i] != 0) {
				int val = arr[y][i];
				arr[y][i] = 0;
				if(val > 1) {
					boom(y, i, val);
				}
				break;
			}
		}
	}

	private static void boom(int y, int x, int val) {
		int[] dx = {0, 0, 1, -1};
		int[] dy = {1, -1, 0, 0};
		
		for (int dir = 0; dir < 4; dir++) {
			int cpx = x;
			int cpy = y;
			for (int idx = 1; idx < val; idx++) {
				cpy += dy[dir];
				cpx += dx[dir];
				if(cpx >= 0 && cpx < w && cpy >= 0 && cpy <h) {
					int val2 = arr[cpy][cpx];
					arr[cpy][cpx] = 0;
					if(val2 > 1) {
						boom(cpy, cpx, val2);
					}	
				}
			}
		}
	}
}
