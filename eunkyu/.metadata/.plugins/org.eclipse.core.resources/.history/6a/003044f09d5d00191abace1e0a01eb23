package algorithm;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Main_16918 {
	
	static int r;
	static int c;
	static int n;
	static char[][] arr;
	static int [][] boomMap;
	static int size;
	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		String[] str = br.readLine().split(" ");

		r = Integer.parseInt(str[0]);
		c = Integer.parseInt(str[1]);
		n = Integer.parseInt(str[2]);
		size = r * c;
		arr = new char[r][c];
		for (int i = 0; i < r; i++) {
			String info = br.readLine();
			for (int j = 0; j < c; j++) {
				char val = info.charAt(j);
				arr[i][j] = val;
				if(val == 'O') {
					boomMap[i][j] = 1;
				}
			}
		}
		
		
		print();
		goGame();
	
	
	}

	private static void goGame() {
		/*
		 * 1. 0 초 - 시작(일부 구간에 폭탄 설치)
		 * 2. 1초  다음 1초 동안 봄버맨은 아무것도 하지 않는다. setTime(time + 1)
		 * 3. 2초 setBoom();
		 * 4. 3초 폭탄 폭발
		 * 3 4 반복
		 */
		setTime(); //1초
		
		
		setTime();
		setBoom(); //2초
		
		setTime();
		isBoom();
		
	}
	
	private static void isBoom() {
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				if(arr[i][j] == 'O' && boomMap[i][j] > 3) {
					for (int dir = 0; dir < 4; dir++) {
						
					}
				}
			}
		}
	}

	private static void setBoom() {
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				if(arr[i][j] == '.') {
					arr[i][j] = 'O';
					boomMap[i][j] = 1;
				}
			}
		}
	}

	private static void setTime() {
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				if(boomMap[i][j] != 0) {
					boomMap[i][j] = boomMap[i][j] + 1;
				}
			}
		}
	}

	private static void print() {
		System.out.println();
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				System.out.print(arr[i][j]+" ");
			}
			System.out.println();
		}
	}
	
}

class Boom_16918 {
	int y;
	int x;
	int time;
	
	public Boom_16918(int y, int x, int time) {
		super();
		this.y = y;
		this.x = x;
		this.time = time;
	}
	
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getTime() {
		return time;
	}
	public void setTime(int time) {
		this.time = time;
	}
}