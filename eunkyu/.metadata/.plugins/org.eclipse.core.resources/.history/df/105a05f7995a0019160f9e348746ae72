package algorithm;

public class Rotate {
	
	static int n = 3;
	static int[][] arr;
	
	public static void main(String[] args) {
		arr = new int[n][n];
		int val = 1;
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				arr[i][j] = val++;
			}
		}
		print();
		rotate90();
	
	}

	private static void rotate90() {
		// TODO Auto-generated method stub
		
	}

	private static void print() {
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				System.out.print(arr[i][j]+" ");
			}
			System.out.println();
		}
	}
}
