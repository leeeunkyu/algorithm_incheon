package algorithm;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Solution_5644 {
	
	static final int MAP_SIZE = 10;//∏  ªÁ¿Ã¡Ó
	static int m;	//√— ¿ÃµøΩ√∞£
	static int a;	//bc¿« ∞πºˆ
	
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int testCase = Integer.parseInt(br.readLine());
		for (int tc = 1; tc <= testCase; tc++) {
			String[] info = br.readLine().split(" ");
			m = Integer.parseInt(info[0]);
			a = Integer.parseInt(info[1]);
			
			for (int i = 0; i < m; i++) {
				String[] walkInfoA = br.readLine().split(" ");
				
			}
			
			for (int i = 0; i < m; i++) {
				String[] walkInfoB = br.readLine().split(" ");
				
			}
			
		}
	}
}

class Battery_5644 {
	int y;
	int x;
	int c;
	int p;
	
	public Battery_5644(int y, int x, int c, int p) {
		super();
		this.y = y;
		this.x = x;
		this.c = c;
		this.p = p;
	}
	
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getC() {
		return c;
	}
	public void setC(int c) {
		this.c = c;
	}
	public int getP() {
		return p;
	}
	public void setP(int p) {
		this.p = p;
	}
	
}