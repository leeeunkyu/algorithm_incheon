package algorithm;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;

public class Main_15683 {
	// 1은 한방향
	// 2는 양방향
	// 3은 90도
	// 4는 세방향
	// 5는 네방향
	static int N; 	//세로크기  N < 8
	static int M;	//가로크기 M < 8
	static int arr[][];
	static int visited[][];
	static ArrayList<Cctv> cctyList = new ArrayList<>(); //size < 8
	static int rotSize[] = {4, 2, 4, 4, 1}; //cctv type마다 회전해야 하는 수
	
	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		String str[] = br.readLine().split(" ");
		N = Integer.parseInt(str[0]);
		M = Integer.parseInt(str[1]);
		arr = new int[N][M];
		visited = new int[N][M];
		for (int i = 0; i < N; i++) {
			String tempStr[] = br.readLine().split(" ");
			for (int j = 0; j < tempStr.length; j++) {
				arr[i][j] = Integer.parseInt(tempStr[j]);
				if(arr[i][j] != 0 && arr[i][j] != 6) {
					//cctv y, x 좌표 type (type 은 1을 빼고 넣어야 rotsize랑 대칭이 된다
					cctyList.add(new Cctv(j, i, (arr[i][j] - 1)));
				}
					
			}
		}
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr[i].length; j++) {
				System.out.print(arr[i][j]);
			}
			System.out.println();
		}
		
		dfs(0);		
		System.out.println("@@@@@@@@@@@@@@@@@@@@@@@@@@@@");
		for (int i = 0; i < visited.length; i++) {
			for (int j = 0; j < visited[i].length; j++) {
				System.out.print(visited[i][j]);
			}
			System.out.println();

		}
		
	}
	private static void dfs(int i) {
		if(i == cctyList.size()) {
			return ;
		}
		Cctv cctv = cctyList.get(i);
		int type = cctv.getType();
		for (int j = 0; j < rotSize[type]; j++) {
			if(type == 0) {
				update(j, cctv);
			}else if(type == 1) {
				update(j, cctv);
				update(j + 2, cctv);

			}else if(type == 2) {
				update(j, cctv);
				update(j + 1, cctv);

			}else if(type == 3) {
				update(j, cctv);
				update(j + 1, cctv);
				update(j + 2, cctv);

			}else if(type == 4) {
				update(j, cctv);
				update(j + 1, cctv);
				update(j + 2, cctv);
				update(j + 3, cctv);

			}
			dfs(++i);
		}
	}
	
	private static void update(int dir, Cctv cctv) {
		dir = dir % 4; //방향을 동, 서, 남, 북  4가지로만 나타내기 위해

		if(dir == 0) {
		// 오른쪽으로
			for (int i = cctv.getX() + 1; i < M; i++) {
				if(arr[cctv.getY()][i] == 6) break;
				visited[cctv.getY()][i] = -1;
			}
		}else if(dir == 1) {
			// 위쪽으로
			for (int i = cctv.getY() - 1; i >= 0; i--) {
				if(arr[i][cctv.getX()] == 6) break;
				visited[i][cctv.getX()] = -1;
			}
		}else if(dir == 2) {
			// 왼쪽으로
			for (int i = cctv.getX() - 1; i >= 0; i--) {
				if(arr[cctv.getY()][i] == 6) break;
				visited[cctv.getY()][i] = -1;
			}
		}else if(dir == 3) {
			// 아래로
			for (int i = cctv.getY() + 1; i < N; i++) {
				if(arr[i][cctv.getX()] == 6) break;
				visited[i][cctv.getX()] = -1;
			}
		}
		
	}
}

class Cctv {
	
	private int x;
	private int y;
	private int type;
	
	public Cctv() {
		super();
	}
	
	public Cctv(int x, int y, int type) {
		super();
		this.x = x;
		this.y = y;
		this.type = type;
	}
	
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}
	
}
