package algorithm;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.LinkedList;
import java.util.Queue;

//초기 상태에서 줄기세포는 비활성 상태
//생명력 수치 X -> X시간동안 비 활성 그 후 활성
//활성 후 X시간이 지남 죽음 -> 죽은상태여도 그리드 크기는 차지
//1시간동안 상 하 좌 우 번식
//번식하는 방향에 이미 세포가 있으면 추가적으로 번식X
public class Solution_5653 {
	
	static int n;
	static int m;
	static int k;
	
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int testCase = Integer.parseInt(br.readLine());
		String[] info = br.readLine().split(" ");
		n = Integer.parseInt(info[0]); //세로크기
		m = Integer.parseInt(info[1]); //가로크기
		k = Integer.parseInt(info[2]); //시간
		Queue<Cell_5653> cells = new LinkedList<Cell_5653>();
		
		int[][] arr = new int[n][m];
		for (int i = 0; i < n; i++) {
			String[] arrInfo = br.readLine().split(" ");
			for (int j = 0; j < m; j++) {
				int val = Integer.parseInt(arrInfo[j]);
				arr[i][j] = val;
				if (val != 0) {
					cells.add(new Cell_5653(i, j, val, 0));
				}
			}
		}
		
		new Solution_5653().goCell(arr, cells);
	}

	private void goCell(int[][] arr, Queue<Cell_5653> cells) {
		for (int i = 0; i < k; i++) {
			
		}
	}
}

class Cell_5653 {
	int y;
	int x;
	int life; //생명력
	int num; //몇 시간
	
	public Cell_5653(int y, int x, int life, int num) {
		super();
		this.y = y;
		this.x = x;
		this.life = life;
		this.num = num;
	}
	
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getLife() {
		return life;
	}
	public void setLife(int life) {
		this.life = life;
	}
	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}	
}
