package algorithm;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

//13:15
public class Main_17136 {
	
	static int n;
	static int[][] arr;
	static boolean[][] visited;
	static int[] nums = {0, 5, 5, 5 ,5, 5};
	static int total;
	static int drawNum;
	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		arr = new int[n][n];
		total = 0;
		for (int i = 0; i < n; i++) {
			String[] str = br.readLine().split(" ");
			for (int j = 0; j < n; j++) {
				int val = Integer.parseInt(str[j]);
				arr[i][j] = val;
				if(val == 1)
					++total;
			}
		}
		
		goGame();
	}

	private static void goGame() {
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				if(arr[i][j] == 1 && !visited[i][j]) {
					setMap(i, j);
				}
			}
		}
	}

	private static void setMap(int y, int x) {
		for (int i = 5; i >= 1; i--) {
			if(nums[i] > 0 && check(y, x, i)) {
				draw(y, x, i);
				nums[i] -= 1;
				
				erase(y, x, i);
			}
		}
	}

	private static void erase(int y, int x, int idx) {
		for (int i = y; i < y + idx; i++) {
			for (int j = x; j < x + idx; j++) {
				visited[i][j] = false;
			}
		}
	}

	private static void draw(int y, int x, int idx) {
		for (int i = y; i < y + idx; i++) {
			for (int j = x; j < x + idx; j++) {
				visited[i][j] = true;
			}
		}
	}

	private static boolean check(int y, int x, int idx) {
		//idx 1, 2, 3....5
		for (int i = y; i < y + idx; i++) {
			for (int j = x; j < x + idx; j++) {
				if(visited[i][j] || arr[i][j] != 1) {
					return false;
				}
			}
		}
		return true;
	}
}
