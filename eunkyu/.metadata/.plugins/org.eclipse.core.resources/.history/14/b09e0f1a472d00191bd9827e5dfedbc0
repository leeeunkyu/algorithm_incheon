package algorithm;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Solution_4014 {
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		StringBuilder sb = new StringBuilder();
		
		int testCase = Integer.parseInt(br.readLine());
		
		for (int tc = 1; tc <= testCase; tc++) {
			String str[] = br.readLine().split(" ");		
			int n = Integer.parseInt(str[0]);
			int x = Integer.parseInt(str[1]);
			
			int arr [][] = new int [n][n];
			for (int i = 0; i < arr.length; i++) {
				String temp [] = br.readLine().split(" ");
				for (int j = 0; j < arr[i].length; j++) {
					arr[i][j] = Integer.parseInt(temp[j]); 
				}
			}
			
			int cnt = setRunway(arr, x, n);
			sb.append("#"+tc+" "+cnt+"\n");
		}
		
		System.out.println(sb);
	}

	private static int setRunway(int[][] arr, int x, int n) {
		int cnt = 0;
		System.out.println("n: "+n);
		for (int i = 0; i < n; i++) {
			//가로줄.. arr[n][n] - arr[n][n+1] - arr[n][n+2} - ...
			int sub = 0;
/*			for (int j = 0; j < n; j++) {
				sub = 0;
				if(j + 1 < n)
					sub = arr[i][j] - arr[i][j+1];
				if(sub > 1) {
					break;
				}
				if(sub == 1 || sub == -1) {
					boolean dir = true;
					if(sub > 0) {
						dir = true;
					} else {
						dir = false;
					}
					System.out.println("i: "+i+" j: "+j+" dir: "+dir);
					if(setSlope(arr, i, j, x, true, dir)) {
						//만약 현재위치 + 경사로길이가 범위를 벗어나지않고, setSlope(경사로 설치기준을 만족한다면 계속 탐색)
					}else {
						System.out.println("false i: "+i+" j: "+j+" dir: "+dir);
						break;
					}					
				}
				if(j == n - 1) {
					System.out.println("cnt++");
					cnt++;
				}
			}*/
			
			//세로줄 arr[n][n] - arr[n+1][n] - arr[n+2][n] - ..
			for (int j = 0; j < n; j++) {
  				sub = 0;
				if(j + 1 < n)
					sub = arr[j][i] - arr[j+1][i];
				if(sub > 1) {
					System.out.println("i: "+i+" j: "+j);
					break;
				}
				if(sub == 1 || sub == -1) {
					boolean dir = true;
					if(sub > 0) {
						dir = true;
					} else {
						dir = false;
					}
					System.out.println("i: "+i+" j: "+j);
					if(setSlope(arr, i, j, x, false, dir)) {
						//만약 현재위치 + 경사로길이가 범위를 벗어나지않고, setSlope(경사로 설치기준을 만족한다면 계속 탐색)
					}else {
						//System.out.println("false i: "+i+" j: "+j+" dir: "+dir);
						System.out.println("false i: "+i+" j: "+j);
						break;
					}		
				}				
				if(j == n - 1) {
					//System.out.println("cnt i: "+i+" j: "+j);
					cnt++;
				}
			}
		}
		
		return cnt;
	}

	private static boolean setSlope(int[][] arr, int i, int j, int slopeLength, boolean type, boolean dir) {
		int x = 0;	//j + 1 + x < n
		int y = 0;
		
		int val = 0;
		if(type) {
			if(dir) {
				x = j + 1;	//j + 1 + x < n
				y = i;
				if(dir && x + (slopeLength - 1) >= arr.length) {
					System.out.println("1");
					return false;
				}
				if(x >= arr.length) {
					System.out.println("3");
					return false;
				}
				val = arr[y][x];
/*				j + x + 1< n
*/				int l = 0;
				for (; l < slopeLength && x < arr.length; x++) {
					if(val != arr[y][x]) {
						return false;
					} else {
						val = arr[y][x];
					}
					l++;
				}	
			} else {
				x = j;	//j - 1 - x >= 0
				y = i;
			
				if(!dir && x - (slopeLength - 1) < 0) {
					System.out.println("2 x:"+x);
					return false;
				}
				if(x <= 0) {
					System.out.println("3");
					return false;
				}
				val = arr[y][x];
				int l = 0;
				for (; l < slopeLength && x >= 0; x--) {
					System.out.println("dd: "+slopeLength+" l: "+l);
					if(val != arr[y][x]) {
						System.out.println("4");
						return false;
					} else {
						val = arr[y][x];
					}
					l++;
				}	
			}
			
		} else {
			if(dir) {
				
				x = i;	//j + 1 + x < n
				y = j + 1;
				//6 - (2 - 1) = 5 >= 6
				if(dir && y - (slopeLength - 1) >= arr.length) {
					System.out.println("1");
					return false;
				}
				if(y >= arr.length) {
					System.out.println("3: "+y);
					return false;
				}
				val = arr[y][x];
/*				j + x + 1< n
*/				int l = 0;
				for (; l < slopeLength && y < arr.length; y++) {
/*					System.out.println("dd "+slopeLength+"y: "+y);
*/					if(val != arr[y][x]) {
/*						System.out.println("val: "+val+" arr[y][x]: "+arr[y][x]);
*/						return false;
					} else {
						val = arr[y][x];
					}
					l++;
				}	
			} else {
				x = i;	//j - 1 - x >= 0
				y = j;
				if(!dir && y - (slopeLength - 1)< 0) {
					System.out.println("2");
					return false;
				}
				if(y <= 0)
					return false;
				val = arr[y][x];
				int l = 0;
				for (; l < slopeLength && y >= 0; y--) {
					if(val != arr[y][x]) {
						return false;
					} else {
						val = arr[y][x];
					}
					l++;
				}	
			}
		}
		return true;
	}
}
