package algorithm;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.LinkedList;
import java.util.Queue;

public class Solution_5656 {

	static int n;
	static int width;
	static int hight;
	static int[][] arr;
	static int[][] copyMap;
	static int min;
	//0은 빈 공간 그 외의 숫자는 벽돌
	//상 하 좌 우로 숫자 -1 만큼 제거
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int testCase = Integer.parseInt(br.readLine());
		StringBuilder sb = new StringBuilder();
		
		for (int tc = 0; tc <= testCase; tc++) {
			String[] str = br.readLine().split(" ");
			
			n = Integer.parseInt(str[0]);
			width = Integer.parseInt(str[1]);
			hight = Integer.parseInt(str[2]);
			
			arr = new int[hight][width];
			copyMap = new int[hight][width];
			
			for (int i = 0; i < hight; i++) {
				String[] info = br.readLine().split(" ");
				for (int j = 0; j < width; j++) {
					arr[i][j] = Integer.parseInt(info[j]);
					copyMap[i][j] = Integer.parseInt(info[j]);
				}
			}
			min = Integer.MAX_VALUE;
			goGame(0);
			sb.append("#"+tc+" "+min);
		}
		System.out.println(sb);
	}
	private static void goGame(int cnt) {
		if(cnt == n) {
			int blockCnt = 0;
			for (int i = 0; i < hight; i++) {
				for (int j = 0; j < width; j++) {
					if(arr[i][j] != 0) {
						blockCnt += 1;
					}
				}
			}
			if(min > blockCnt) {
				min = blockCnt;
				System.out.println(min);
			}
			return;
		}
		for (int i = 0; i < width; i++) {
			dropBall(i);
			copy(true);
			goGame(cnt + 1);
			copy(false);
		}
		
	}
	private static void copy(boolean type) {
		if (type) {
			for (int i = 0; i < hight; i++) {
				for (int j = 0; j < width; j++) {
					copyMap[i][j] = arr[i][j];
				}
			}
		} else {
			for (int i = 0; i < hight; i++) {
				for (int j = 0; j < width; j++) {
					arr[i][j] = copyMap[i][j];
				}
			}
		}
	}
	private static void dropBall(int idx) {
		int h = 0;
		int val = 0;
		for (int i = 0; i < h; i++) {
			val = arr[i][idx];
			if(val != 0) { 
				arr[i][idx] = 0;
				h = i;
				break;
			}
		}
		
		if(val > 1) {
			boom(val, h, idx);
		}
		reset();
	}
	private static void reset() {
		System.out.println("??");
		for (int x = 0; x < width; x++) {
			Queue<Integer> q = new LinkedList<Integer>();
			for (int y = 0; y < hight; y++) {
				int val = arr[y][x];
				if(val != 0) {
					q.add(val);
					arr[y][x] = 0;
				}
			}
			int tempY = hight - 1;
			while(!(q.isEmpty())) {
				arr[tempY][x] = q.poll();
				tempY--;
			}
		}
	}
	private static void boom(int val, int h, int idx) {
		int y = h;
		int x = idx;
		int[] dx = {0, 0, 1, -1};
		int[] dy = {1, -1, 0, 0};
		for (int v = 0; v < val - 1; v++) {			
			for (int i = 0; i < 4; i++) {
				
				int nextX = x + dx[i];
				int nextY = y + dy[i];
				
				for (int j = 0; j < v; j++) {
					nextX += dx[i];
					nextY += dy[i];
				}
				
				if(nextX >= 0 && nextX < width && nextY >= 0 && nextY < hight) {
					if(arr[nextY][nextX] > 1) {
						boom(arr[nextY][nextX], nextY, nextX);
					} else {
						arr[nextY][nextX] = 0;
					}
				}
			}	
		}
	}
	
}
