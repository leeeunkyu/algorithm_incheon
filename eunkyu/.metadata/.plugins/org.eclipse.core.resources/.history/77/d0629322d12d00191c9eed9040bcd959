package algorithm;

public class Main_1592 {
	public static void main(String[] args) {
		
	}
}

class People {
	
	private int num;
	private int ball;
	
	public People(int num, int ball) {
		super();
		this.num = num;
		this.ball = ball;
	}
	
	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}
	public int getBall() {
		return ball;
	}
	public void setBall(int ball) {
		this.ball = ball;
	}
}
